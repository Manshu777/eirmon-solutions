{
  "expo": {
    "name": "neerisalon",
    "slug": "neerisalon",
    "version": "1.0.3",
    "orientation": "portrait",
    "projectId": "2c5035aa-d09b-4226-8957-0aa12d5d6429",
    "icon": "./assets/images/icon.png",
    "scheme": "neerisalon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
       "buildNumber": "3",
      "supportsTablet": true,
      "bundleIdentifier": "com.neerisalon.eirmonsolutions",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "android": {
      "googleServicesFile": "./google-services.json"
    },
      "edgeToEdgeEnabled": true,
      "package": "com.neerisalon.eirmonsolutions"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
        "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2c5035aa-d09b-4226-8957-0aa12d5d6429"
      }
    }
  }
}
